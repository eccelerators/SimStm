array array_name_zsz 0
var var_name_zsz 10

testArrayZeroSize:
proc
	call $startStandardTestShell
	
	array size array_name_zsz var_name_zsz
	var verify var_name_zsz 0 $MAX
	
	call $endStandardTestShell
end proc

testArrayGetOutPos:
proc
	array get array_name 3 var_name
end proc

testArraySetOutPos:
proc
	array set array_name 3 $var_name
end proc