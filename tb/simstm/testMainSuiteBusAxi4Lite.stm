include "Common/Common.stm"

include "Bus/bus.stm"

include "TestSuites/TestSuiteBusAxi4Lite.stm"

testMainSuiteBusAxi4Lite:
proc
    call $initBase
        	
	verbosity $INFO_2
	trace $TRACE_OFF
	resume $RESUME_ON_ALL_ERRORS
	signal write out_signal_init_dut 1
	wait 1000
	signal write out_signal_init_dut 0
	log message $INFO "Main test suite Bus Axi4Lite started"

	call $testSuiteBusAxi4Lite	
	
	log message $INFO "Main test suite Bus Axi4Lite ended"
	wait 1000
	finish
end proc
