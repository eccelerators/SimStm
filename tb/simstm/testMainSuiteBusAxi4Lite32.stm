include "Common/Common.stm"

include "Bus/bus32.stm"

include "TestSuites/TestSuiteBusAxi4Lite32.stm"

testMainSuiteBusAxi4Lite32:
proc
    call $initBase
        	
	verbosity $INFO_2
	trace $TRACE_OFF
	resume $RESUME_ON_ALL_ERRORS
	signal write out_signal_init_dut 1
	wait 1000
	signal write out_signal_init_dut 0
	log message $INFO "Main test suite Bus Axi4Lite 32 started"

	call $testSuiteBusAxi4Lite32	
	
	log message $INFO "Main test suite Bus Axi4Lite 32 ended"
	wait 1000
	finish
end proc
