
--  create a deep  while loop.
CL1:
WHILE $CNT < 500
  WHILE $CNT1 < 50
    WHILE $CNT2 < 50
      WHILE $CNT3 < 10
        WHILE $CNT4 < 10
          WHILE $CNT5 < 4
            WHILE $CNT6 < 2
              ADD_VAR CNT6 1
            END_WHILE
            EQU_VAR CNT6 0
            ADD_VAR CNT5 1
          END_WHILE
          EQU_VAR CNT5 0
          ADD_VAR CNT4 1
        END_WHILE
        EQU_VAR CNT4 0
        ADD_VAR CNT3 1
      END_WHILE
      EQU_VAR CNT3 0
      ADD_VAR CNT2 1
    END_WHILE
    EQU_VAR CNT2 0
    ADD_VAR CNT1 1
  END_WHILE
  EQU_VAR CNT1 0
  ADD_VAR CNT 1
END_WHILE

EQU_VAR CNT 0

RETURN_CALL
