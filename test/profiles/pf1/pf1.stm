--  example stimulus file
MESSAGES_ON

DEFINE_VAR DATA   -20
DEFINE_VAR ADDR1  4
DEFINE_VAR ADDR2  5


RESET_SYS

LOOP 5
  --MESSAGES_ON
  WRITE_DUT $ADDR1 $DATA  "  Write  addr: $ADDR1  with data: $DATA
  --MESSAGES_OFF
  READ_PINS 0
  VERIFY $DATA
  ADD_VAR DATA 1

  WRITE_DUT $ADDR2 $DATA
  READ_PINS 1
  VERIFY $DATA
  ADD_VAR DATA 1
  CALL $CL1
END_LOOP

FINISH

INCLUDE "stm/include.stm
