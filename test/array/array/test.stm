array array_name 3

var var_name 10

array set array_name 0 3
array get array_name 0 var_name
log 0 "var_name: $var_name"

if $var_name != 3
    abort
end if

equ var_name 1234
array set array_name 1 $var_name
array get array_name 1 var_name
log 0 "var_name: $var_name"

if $var_name != 1234
    abort
end if

array get array_name 0 var_name
log 0 "var_name: $var_name"

if $var_name != 3
    abort
end if

finish
