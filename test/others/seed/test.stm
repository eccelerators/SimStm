var random_number 0

var seed_set 5
var random_min 0
var random_max 10


Main:
proc
	seed 1
	loop 5
	    random random_number 0 10
	    log message 0 "random_number: {}" $random_number
	end loop
	
	seed $seed_set
	loop 5
	    random random_number $random_min $random_max
	    log message 0 "random_number: {}" $random_number
	end loop	
	
	finish
end proc