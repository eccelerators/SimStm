const marker_1 1
const marker_15 15

const val_marker_low 0
const val_marker_high 1

const val_trace_off 0
const val_trace_on 1

Main:
proc
    trace 1
	wait 100
	marker 0 1
	marker $marker_1 $val_marker_high
	trace 0
    wait 1000
    marker 0 0
    marker $marker_1 $val_marker_low

	trace $val_trace_on
	wait 100
	marker 14 1
	marker $marker_15 $val_marker_high
	wait 100
	marker 14 0
	marker $marker_15 $val_marker_low
	trace $val_trace_off
    wait 100
	marker 14 1
	marker $marker_15 $val_marker_high
	wait 100
	marker 14 0
	marker $marker_15 $val_marker_low
		
	finish
end proc